[mysqld]
secure-file-priv =
event_scheduler = ON
max_connections = 200
max_heap_table_size = 256M
tmp_table_size = 256M
key_buffer_size = 256M
query_cache_size = 256M
query_cache_limit = 4M
join_buffer_size = 512k
table_open_cache = 64
innodb_buffer_pool_size = 512M
innodb_flush_log_at_trx_commit = 2
