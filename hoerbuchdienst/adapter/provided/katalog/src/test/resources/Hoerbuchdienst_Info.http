// Informationen über einen Track eines Hörbuchs abrufen
GET {{host}}/{{api-version}}/katalog/{{titelnummer}}/track/01_Halt_die_Wolken_fest.mp3
X-Bookworm-Mandant: {{mandant}}
X-Bookworm-Hoerernummer: {{hoerernummer}}

